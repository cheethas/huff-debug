{
  "name": "huff-debug",
  "version": "0.0.1",
  "description": "Hevm based debugger support for hardhat-huff projects",
  "repository": "github:huff-language/huff-debug",
  "author": "maddiaa | cheethas",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "huff-language",
    "huff",
    "hardhat",
    "hardhat-plugin",
    "nissan skyline gtr r34"
  ],
  "scripts": {
    "lint:fix": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/src/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@types/chai": "^4.1.7",
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^8.10.38",
    "chai": "^4.2.0",
    "hardhat": "^2.0.0",
    "hardhat-huff": "^1.0.19",
    "mocha": "^7.1.2",
    "prettier": "2.0.5",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "hardhat": "^2.0.0"
  },
  "dependencies": {
    "command-exists": "^1.2.9",
    "glob": "^8.0.3"
  }
}
